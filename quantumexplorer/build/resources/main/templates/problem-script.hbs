<script type="application/javascript">
    function generateProblem(type, element, title) {
        $(document).ready(function () {
            $.get("/problems/generate?type=" + type, function (problemHtml) {
                $("#" + element).html(problemHtml);
                $("#problem-div").prepend("<h4 class='bold uk-margin-small-top'>" + title + "</h4>")

                MathJax.Hub.Queue(["Typeset", MathJax.Hub, document.getElementById("math-txt")]);
            });
        });
    }
</script>