{{> header.hbs }}

<div>
    <div class="uk-margin-small-top uk-section uk-section-large">
        <div>
            <h2 class="moderate-bold uk-text-center">Practice</h2>

            {{#each generators}}
                <div class="uk-width-1-4 uk-margin-auto">
                    <h3 class="uk-text-center">{{first}}</h3>

                    <ul class="uk-list uk-list-bullet">
                        {{#each second}}
                            <li>
                                <a href="{{& second}}">{{first}}</a>
                            </li>
                        {{/each}}
                    </ul>

                </div>
            {{/each}}
        </div>
    </div>
</div>

{{> footer.hbs }}